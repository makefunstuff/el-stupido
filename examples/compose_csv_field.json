{
  "app": "csv-field",
  "capabilities": ["io_write"],
  "nodes": [
    {"id": "line", "use": "arg_str", "params": {"index": 1}},
    {"id": "idx", "use": "arg_num", "params": {"index": 2}},
    {"id": "delim", "use": "const_str", "params": {"value": ","}},
    {"id": "field", "use": "split_nth", "bind": {"text": "line", "delim": "delim", "index": "idx"}},
    {"id": "trimmed", "use": "trim_str", "bind": {"text": "field"}},
    {"id": "out", "use": "print_str", "bind": {"value": "trimmed"}}
  ]
}
