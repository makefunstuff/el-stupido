{
  "app": "branch-repeat",
  "capabilities": ["io_write"],
  "nodes": [
    {"id": "a", "use": "const_num", "params": {"value": 7}},
    {"id": "b", "use": "const_num", "params": {"value": 3}},
    {"id": "gt_ab", "use": "gt", "bind": {"lhs": "a", "rhs": "b"}},
    {"id": "eq_ab", "use": "eq_num", "bind": {"lhs": "a", "rhs": "b"}},
    {"id": "not_eq", "use": "not_bool", "bind": {"value": "eq_ab"}},
    {"id": "cond", "use": "and_bool", "bind": {"lhs": "gt_ab", "rhs": "not_eq"}},
    {"id": "yes", "use": "const_str", "params": {"value": "A>B "}},
    {"id": "no", "use": "const_str", "params": {"value": "A<=B "}},
    {"id": "chosen", "use": "select_str", "bind": {"cond": "cond", "then": "yes", "else": "no"}},
    {"id": "times", "use": "const_num", "params": {"value": 3}},
    {"id": "msg", "use": "repeat_str", "bind": {"text": "chosen", "times": "times"}},
    {"id": "out", "use": "print_str", "bind": {"value": "msg"}}
  ]
}
