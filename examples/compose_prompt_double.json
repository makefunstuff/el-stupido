{
  "app": "prompt-double",
  "capabilities": ["io_read", "io_write"],
  "nodes": [
    {"id": "prompt", "use": "read_stdin", "params": {"prompt": "Enter a number: "}},
    {"id": "n", "use": "parse_num", "bind": {"text": "prompt"}},
    {"id": "two", "use": "const_num", "params": {"value": 2}},
    {"id": "double", "use": "mul", "bind": {"lhs": "n", "rhs": "two"}},
    {"id": "label", "use": "const_str", "params": {"value": "double = "}},
    {"id": "double_str", "use": "to_string", "bind": {"value": "double"}},
    {"id": "message", "use": "concat", "bind": {"left": "label", "right": "double_str"}},
    {"id": "out", "use": "print_str", "bind": {"value": "message"}}
  ]
}
