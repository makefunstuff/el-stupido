{
  "app": "is-even",
  "capabilities": ["io_write"],
  "nodes": [
    {"id": "n", "use": "arg_num", "params": {"index": 1}},
    {"id": "two", "use": "const_num", "params": {"value": 2}},
    {"id": "rem", "use": "mod_num", "bind": {"lhs": "n", "rhs": "two"}},
    {"id": "zero", "use": "const_num", "params": {"value": 0}},
    {"id": "is_even", "use": "eq_num", "bind": {"lhs": "rem", "rhs": "zero"}},
    {"id": "yes", "use": "const_str", "params": {"value": "even"}},
    {"id": "no", "use": "const_str", "params": {"value": "odd"}},
    {"id": "result", "use": "select_str", "bind": {"cond": "is_even", "then": "yes", "else": "no"}},
    {"id": "out", "use": "print_str", "bind": {"value": "result"}}
  ]
}
