{
  "app": "greet",
  "capabilities": ["io_write"],
  "nodes": [
    {
      "id": "arg1",
      "use": "arg_str",
      "params": {"index": 1}
    },
    {
      "id": "hello",
      "use": "const_str",
      "params": {"value": "Hello, "}
    },
    {
      "id": "greeting",
      "use": "concat",
      "bind": {"left": "hello", "right": "arg1"}
    },
    {
      "id": "print_greeting",
      "use": "print_str",
      "bind": {"value": "greeting"}
    }
  ]
}