fn product([int start, int end]) {
  return (start...end).reduce((a,b) => a*b)
}
summation_toN([int n]) = '(1..=n)'.
printFactorialOfNumber([integer number]) = 'fact(' + (number)+ '')'
fn forLoopPrintingFactorials() {
  '(1..=n)' { print((i),' ', fact(i)) }
}

fn main() {
  printFactorialOfNumber for i in (product(1, n))
  fact = product
  summationToN = 0
}
